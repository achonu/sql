USE sql_store;

SELECT *
FROM customers
WHERE customer_id = 1;

SELECT *
FROM customers
WHERE points > 3000;

SELECT *
FROM customers
WHERE birth_date > ‘1990-01-01’;

SELECT *
FROM customers
WHERE birth_date > ‘1990-01-01’ AND points > 1000;

SELECT * 
FROM order_items
WHERE order_id = 6 AND unit_price * quantity > 30;

SELECT *
FROM customers
WHERE state = ‘VA’;

SELECT *
FROM customers
WHERE state != ‘VA’;



